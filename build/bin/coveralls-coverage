#!/bin/bash

echo "FIXME: Skipping coverage for now"
exit 0

go get github.com/go-playground/overalls && go get github.com/mattn/goveralls

overalls -project=github.com/onosproject/ran-simulator -covermode=count -ignore=".git,vendor,models,tools"
goveralls -coverprofile=overalls.coverprofile -service travis-ci
